{
  "name": "starbound-autocomplete-json",
  "main": "./src/main",
  "version": "0.0.0",
  "description": "A slightly modified version of the autocomplete-json package for starbound modding.",
  "license": "MIT",
  "repository": "https://github.com/C0bra5/starbound-autocomplete-json",
  "activationHooks": [
    "language-json:grammar-used",
    "language-json-comments:grammar-used"
  ],
  "consumedServices": {
    "jsonschema.provider": {
      "versions": {
        "1.0.0": "consumeJsonSchemaProviders"
      }
    },
    "jsonautocomplete.provider": {
      "versions": {
        "1.0.0 ": "consumeJsonProposalProviders"
      }
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "file-url": "^1.1.0",
    "jaro-winkler": "^0.2.6",
    "lodash": "4.17.4",
    "minimatch": "^3.0.0",
    "npm-package-lookup": "^1.0.0",
    "packagist-package-lookup": "^1.0.0",
    "query-string": "^3.0.0",
    "tokenizer2": "^1.2.0",
    "uri-js": "^2.1.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.16.0"
  },
  "optionalDependencies": {},
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "jsonschema.provider": {
      "versions": {
        "1.0.0": "provideJsonSchemaProviders"
      }
    },
    "jsonautocomplete.provider": {
      "versions": {
        "1.0.0": "provideProposalProviders"
      }
    }
  },
  "readme": "Based on the [autocomplete-json](https://atom.io/packages/autocomplete-json) package, this is a modified version of the original package made to help with starbound mod developpement.\n\nThis package is part of the starbound modding package suite for atom, all plugins are maintained and modified by [C0bra5](https://atom.io/users/C0bra5).\n\nThis plugin is till a work in progress.",
  "readmeFilename": "readme.md",
  "scripts": {
    "lint": "eslint src/** --config .eslintrc"
  }
}
